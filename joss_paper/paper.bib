	Keywords = {galaxies: star clusters, Galaxy: center, Galaxy: kinematics and dynamics, methods: numerical, stellar dynamics},
	Month = oct,
	Pages = {1082-1093},
	Title = {{Evolution of Star Clusters near the Galactic Center: Fully Self-Consistent N-Body Simulations}},
