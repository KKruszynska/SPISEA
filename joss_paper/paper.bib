	Doi = {10.1111/j.1365-2966.2006.11383.x},
	Eprint = {astro-ph/0612257},
	Journal = {\mnras},
	Keywords = {atmospheric effects, methods: data analysis, methods: observational, techniques: spectroscopic, infrared: general},
	Month = mar,
	Pages = {1099-1105},
	Title = {{A method to remove residual OH emission from near-infrared spectra}},
