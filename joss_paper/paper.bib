	Keywords = {Stars: Binaries: Close, Stars: Blue Stragglers, Galaxy: Globular Clusters: General, Galaxy: Open Clusters and Associations: Individual: Name: Arches, Galaxy: Open Clusters and Associations: Individual: Name: Quintuplet cluster, Stars: Evolution},
