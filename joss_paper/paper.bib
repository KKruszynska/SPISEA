	Eid = {A62},
	Eprint = {1407.4664},
	Journal = {\aap},
	Keywords = {methods: data analysis, technique: radial velocity, astrometry, parallaxes, proper motions, reference systems},
	Month = oct,
	Pages = {A62},
	Primaryclass = {astro-ph.IM},
	Title = {{Rigorous treatment of barycentric stellar motion. Perspective and light-time effects in astrometric and radial velocity data}},
	Volume = 570,
