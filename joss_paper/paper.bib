	Date-Modified = {2019-08-01 14:07:55 -0700},
	Doi = {10.1016/j.newar.2013.09.001},
	Eprint = {1312.0015},
	Journal = {\nar},
	Month = nov,
	Pages = {123-139},
	Primaryclass = {astro-ph.IM},
	Title = {{The stochastic nature of stellar population modelling}},
	Volume = 57,
	Year = 2013,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {https://doi.org/10.1016/j.newar.2013.09.001},
