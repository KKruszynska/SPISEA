	Author = {{Sch{\"o}del}, R. and {Gallego-Cano}, E. and {Dong}, H. and {Nogueras-Lara}, F. and {Gallego-Calvente}, A.~T. and {Amaro-Seoane}, P. and {Baumgardt}, H.},
