	Journal = {\aap},
	Keywords = {stars: evolution, Cepheids},
	Month = dec,
	Pages = {355-369},
	Primaryclass = {astro-ph.SR},
	Title = {{Scaled solar tracks and isochrones in a large region of the Z-Y plane. II. From 2.5 to 20 M{\sun} stars}},
