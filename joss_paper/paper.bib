	Date-Modified = {2020-04-01 17:19:50 -0700},
	Doi = {10.1086/510377},
	Eprint = {astro-ph/0606443},
	Journal = {\apj},
	Keywords = {Black Hole Physics, Galaxies: Nuclei, ISM: Clouds, Galaxy: Open Clusters and Associations: General, Stars: Kinematics, Astrophysics},
	Month = feb,
