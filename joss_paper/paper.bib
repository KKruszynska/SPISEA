	Doi = {10.1086/382021},
	Eprint = {astro-ph/0312189},
	Journal = {\apj},
	Keywords = {Stars: Binaries: General, Stars: Hertzsprung-Russell Diagram, Stars: Pre-Main-Sequence},
	Month = apr,
	Pages = {741-757},
