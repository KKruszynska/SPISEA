	Doi = {10.1111/j.1365-2966.2006.11078.x},
	Eprint = {astro-ph/0609477},
	Journal = {\mnras},
	Keywords = {stellar dynamics, methods: N-body simulations, galaxies: star clusters},
	Month = dec,
	Pages = {752-758},
	Title = {{Gas expulsion and the destruction of massive young clusters}},
	Volume = 373,
	Year = 2006,
