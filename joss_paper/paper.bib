	Eprint = {1602.01025},
	Journal = {\mnras},
	Keywords = {Clusters, memberships probabilities, statistical methods},
	Month = jan,
	Primaryclass = {astro-ph.SR},
	Title = {{Stellar Open Clusters' Membership Probabilities: an N-Dimensional Geometrical Approach}},
