	Arxivurl = {http://arXiv.org/abs/1110.5049},
	Author = {{Ekstr{\"o}m}, S. and {Georgy}, C. and {Eggenberger}, P. and {Meynet}, G. and {Mowlavi}, N. and {Wyttenbach}, A. and {Granada}, A. and {Decressin}, T. and {Hirschi}, R. and {Frischknecht}, U. and {Charbonnel}, C. and {Maeder}, A.},
