	Date-Modified = {2017-01-19 22:38:09 +0000},
	Doi = {10.1111/j.1365-2966.2012.21200.x},
	Eprint = {1204.6298},
	Journal = {\mnras},
	Keywords = {stars: emission-line, Be, stars: formation, stars: Wolf-Rayet, dust, extinction, H II regions, Galaxy: centre},
