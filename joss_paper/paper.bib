	Archiveprefix = {arXiv},
	Arxivurl = {http://arXiv.org/abs/1303.2321},
	Author = {{Georgy}, C. and {Ekstr{\"o}m}, S. and {Granada}, A. and {Meynet}, G. and {Mowlavi}, N. and {Eggenberger}, P. and {Maeder}, A.},
	Date-Added = {2015-05-21 23:09:47 +0000},
