@article{Hamann:2003yj,
	Abstract = {Model atmospheres form the basis for the interpretation of stellar spectra. A major problem in those model calculations is to establish the temperature stratification from the condition of radiative equilibrium. Dealing with non-LTE models for spherically expanding atmospheres of Wolf-Rayet stars, we developed a new temperature correction method. Its basic idea dates back to 1955 when it was proposed by Uns{\"o}ld for grey, static and plane-parallel atmospheres in LTE. The equations were later generalized to the non-grey case by Lucy. In the present paper we furthermore drop the Eddington approximation, proceed to spherical geometry and allow for expansion of the atmosphere. Finally the concept of an ``approximate lambda operator'' is employed to speed up the convergence. Tests for Wolf-Rayet type models demonstrate that the method works fine even in situations of strong non-LTE.},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {https://ui.adsabs.harvard.edu/abs/2003A%26A...410..993H},
	Author = {{Hamann}, W.-R. and {Gr{\"a}fener}, G.},
	Date-Added = {2019-07-29 09:19:37 -0700},
	Date-Modified = {2019-07-29 09:19:38 -0700},
	Doi = {10.1051/0004-6361:20031308},
	Journal = {\aap},
	Keywords = {stars: winds, outflows, stars: mass-loss, stars: Wolf-Rayet, radiative transfer, stars: atmospheres},
	Month = nov,
	Pages = {993-1000},
	Title = {{A temperature correction method for expanding atmospheres}},
	Volume = 410,
	Year = 2003,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {https://doi.org/10.1051/0004-6361:20031308},
