	Eprint = {astro-ph/0701476},
	Journal = {\apj},
	Keywords = {Infrared: Stars, open clusters and associations: individual ({$\sigma$} Orionis cluster), Stars: Planetary Systems: Protoplanetary Disks, Stars: Formation, Stars: Pre-Main-Sequence},
