	Doi = {10.1093/mnras/stv1437},
	Eprint = {1506.08228},
	Journal = {\mnras},
	Keywords = {magnetic fields, MHD, stars: formation, ISM: kinematics and dynamics, ISM: magnetic fields},
	Month = sep,
