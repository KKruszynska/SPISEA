	Journal = {\apj},
	Keywords = {Black Hole Physics, Galaxy: Center, Stellar Dynamics},
	Month = dec,
	Pages = {1129-1138},
	Title = {{Ejection of Hypervelocity Stars by the (Binary) Black Hole in the Galactic Center}},
