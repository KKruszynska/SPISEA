	Author = {{Landin}, N.~R. and {Mendes}, L.~T.~S. and {Vaz}, L.~P.~R.},
	Date-Added = {2015-04-10 06:39:33 +0000},
	Date-Modified = {2015-04-10 06:39:34 +0000},
	Doi = {10.1051/0004-6361/200913015},
	Eid = {A46},
