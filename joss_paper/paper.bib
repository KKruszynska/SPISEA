	Date-Added = {2018-04-19 16:53:59 +0000},
	Date-Modified = {2018-04-19 16:54:00 +0000},
	Eprint = {1804.06807},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Astrophysics of Galaxies, Astrophysics - Solar and Stellar Astrophysics},
	Month = apr,
	Title = {{An extremely low mid-infrared extinction law toward the Galactic Center and 4\% distance precision to 55 classical Cepheids}},
