	Author = {{Hennebelle}, P. and {Chabrier}, G.},
	Date-Added = {2019-03-03 14:01:02 -0800},
	Date-Modified = {2019-03-03 14:01:03 -0800},
	Doi = {10.1086/589916},
	Eprint = {0805.0691},
	Journal = {\apj},
	Keywords = {ISM: clouds, stars: formation, stars: luminosity function, mass function, turbulence},
	Month = sep,
	Pages = {395-410},
	Title = {{Analytical Theory for the Initial Mass Function: CO Clumps and Prestellar Cores}},
	Volume = 684,
