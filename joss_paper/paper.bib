	Doi = {10.1051/0004-6361:20054198},
	Eprint = {astro-ph/0604619},
	Journal = {\aap},
	Keywords = {galaxies: star clusters, galaxies: Magellanic Clouds, stars: luminosity function, mass function, stars: Hertzsprung-Russell (HR) and C-M diagrams, methods: numerical},
	Month = jun,
	Pages = {155-162},
