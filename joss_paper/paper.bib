	Eprint = {1411.4864},
	Journal = {\mnras},
	Keywords = {stars: kinematics and dynamics, galaxies: star clusters},
	Month = jan,
	Pages = {770-792},
	Title = {{Revealing the escape mechanism of three-dimensional orbits in a tidally limited star cluster}},
