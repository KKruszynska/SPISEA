	Eprint = {1309.0315},
	Journal = {\aap},
	Keywords = {galaxies: star clusters: general, stars: kinematics and dynamics, open clusters and associations: general},
	Month = nov,
	Pages = {A38},
	Title = {{The expansion of massive young star clusters - observation meets theory}},
	Volume = 559,
	Year = 2013,
