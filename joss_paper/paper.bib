	Doi = {10.1046/j.1365-8711.2002.05271.x},
	Eprint = {astro-ph/0112331},
	Journal = {\mnras},
	Keywords = {methods: numerical, methods: statistical, Local Group, galaxies: stellar content, Astrophysics},
	Month = may,
	Number = {1},
	Pages = {91-108},
	Primaryclass = {astro-ph},
	Title = {{Numerical methods of star formation history measurement and applications to seven dwarf spheroidals}},
