	Eprint = {astro-ph/0703025},
	Journal = {\mnras},
	Keywords = {open clusters and associations: individual: NGC5715 , open clusters and associations: individual: Lyng{\aa}4 , open clusters and associations: individual: Lyng{\aa}9 , open clusters and associations: individual: Trumpler23 , open clusters and associations: individual: Trumpler26 , open clusters and associations: individual: Czernik37},
