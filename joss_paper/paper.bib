	Eprint = {1711.06389},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Astrophysics of Galaxies, General Relativity and Quantum Cosmology},
	Month = nov,
	Title = {{Measuring the effects of General Relativity at the Galactic Center with Future Extremely Large Telescopes}},
	Year = 2017,
