	Date-Modified = {2020-04-15 16:05:41 -0700},
	Doi = {10.1088/0004-637X/775/1/58},
	Eid = {58},
	Eprint = {1308.3476},
	Journal = {\apj},
	Keywords = {Galaxy: evolution, stars: abundances, stars: evolution, stars: late-type, stars: solar-type, open clusters and associations: general, open clusters and associations: individual: M 44 NGC 2632 Praesepe, Astrophysics - Solar and Stellar Astrophysics},
	Month = sep,
