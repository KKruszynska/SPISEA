	Date-Added = {2015-11-17 00:06:04 +0000},
	Date-Modified = {2015-11-17 00:06:05 +0000},
	Eprint = {1511.01500},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Astrophysics of Galaxies},
	Month = nov,
	Title = {{The Bottom-Light Present Day Mass Function of the Peculiar Globular Cluster NGC 6535}},
	Year = 2015,
