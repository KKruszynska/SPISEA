	Author = {{St{\o}stad}, M.~N. and {Do}, T. and {Murray}, N. and {Lu}, J.~R. and {Yelda}, S. and {Ghez}, A.~M.},
	Date-Added = {2015-05-14 01:31:44 +0000},
