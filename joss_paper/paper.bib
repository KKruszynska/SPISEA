	Doi = {10.1093/mnrasl/slw061},
	Eprint = {1604.00394},
	Journal = {\mnras},
	Keywords = {stars: formation, kinematics and dynamics, open clusters and associations: general, methods: numerical},
	Month = apr,
	Title = {{Mass segregation in star clusters is not energy equipartition}},
	Year = 2016,
	Bdsk-File-1 = {},
