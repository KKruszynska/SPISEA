	Journal = {\aap},
	Keywords = {ISM: clouds, ISM: structure, turbulence, stars: formation},
	Month = apr,
	Pages = {A89},
	Title = {{Stellar mass spectrum within massive collapsing clumps. II. Thermodynamics and tidal forces of the first Larson core. A robust mechanism for the peak of the IMF}},
