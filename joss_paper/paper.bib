	Eprint = {1410.5540},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Astrophysics of Galaxies},
	Month = oct,
	Title = {{Formation of young massive clusters from turbulent molecular clouds}},
	Year = 2014,
	Bdsk-File-1 = {},
