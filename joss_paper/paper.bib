	Journal = {\aap},
	Keywords = {NUCLEAR REACTIONS, NUCLEOSYNTHESIS, ABUNDANCES, STARS: MASS LOSS, SUPERNOVAE: GENERAL, GALAXY: EVOLUTION, GALAXY: ABUNDANCES, SOLAR NEIGHBOURHOOD, Astrophysics},
