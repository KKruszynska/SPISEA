	Doi = {10.1051/0004-6361/200913757},
	Eid = {A80},
	Eprint = {1107.5199},
	Journal = {\aap},
	Keywords = {Galaxy: bulge, Galaxy: formation, Galaxy: abundances, stars: abundances, stars: atmospheres},
	Month = oct,
	Pages = {A80},
	Title = {{The metallicity distribution of bulge clump giants in Baade's window}},
