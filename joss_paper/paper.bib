	Doi = {10.1086/305350},
	Journal = {\apj},
	Keywords = {RADIATIVE TRANSFER, STARS: EMISSION-LINE, BE, LINE: FORMATION, STARS: MASS LOSS, STARS: WOLF-RAYET, Line: Formation, Radiative Transfer, Stars: Emission-Line, Be, Stars: Mass Loss, Stars: Wolf-Rayet},
