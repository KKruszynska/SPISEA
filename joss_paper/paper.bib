	Author = {{Dougherty}, S.~M. and {Clark}, J.~S. and {Negueruela}, I. and {Johnson}, T. and {Chapman}, J.~M.},
	Date-Added = {2017-07-28 21:54:20 +0000},
	Date-Modified = {2017-07-28 21:54:21 +0000},
	Doi = {10.1051/0004-6361/200913505},
