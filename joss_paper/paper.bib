	Author = {{Larsen}, S.~S. and {Baumgardt}, H. and {Bastian}, N. and {Brodie}, J.~P. and {Grundahl}, F. and {Strader}, J.},
	Date-Added = {2015-03-30 03:21:33 +0000},
