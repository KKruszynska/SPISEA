	Eprint = {1509.07119},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Astrophysics of Galaxies},
	Month = sep,
	Title = {{Possible smoking-gun evidence for initial mass segregation in re-virialized post-gas expulsion globular clusters}},
