	Author = {{Hoversten}, E.~A. and {Glazebrook}, K.},
	Date-Added = {2015-03-23 19:47:34 +0000},
	Date-Modified = {2015-03-23 19:47:35 +0000},
	Doi = {10.1086/524095},
	Eprint = {0711.1309},
	Journal = {\apj},
	Keywords = {Galaxies: Evolution, Galaxies: Stellar Content, Stars: Luminosity Function, Mass Function},
	Month = mar,
	Pages = {163-187},
	Title = {{Evidence for a Nonuniversal Stellar Initial Mass Function from the Integrated Properties of SDSS Galaxies}},
