	Doi = {10.1051/0004-6361/201323124},
	Eid = {A32},
	Eprint = {1403.1736},
	Journal = {\aap},
	Keywords = {protoplanetary disks, stars: protostars, circumstellar matter, open clusters and associations: general},
	Month = may,
	Pages = {A32},
	Title = {{Does the mass distribution in discs influence encounter-induced losses in young star clusters?}},
	Volume = 565,
