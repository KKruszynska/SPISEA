	Doi = {10.1086/307892},
	Eprint = {astro-ph/9905325},
	Journal = {\apj},
	Keywords = {CELESTIAL MECHANICS, STELLAR DYNAMICS, GALAXIES: STAR CLUSTERS, GALAXY: CENTER, METHODS: NUMERICAL, GALAXY: OPEN CLUSTERS AND ASSOCIATIONS: GENERAL, Celestial Mechanics, Stellar Dynamics, Galaxies: Star Clusters, Galaxy: Center, Methods: Numerical, Galaxy: Open Clusters and Associations: General},
	Month = nov,
	Pages = {228-239},
	Title = {{Evaporation of Compact Young Clusters near the Galactic Center}},
	Volume = 525,
	Year = 1999,
