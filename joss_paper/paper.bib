	Doi = {10.1093/mnras/sty3252},
	Eprint = {1805.06614},
	Journal = {\mnras},
	Keywords = {galaxies: dwarf, galaxies: evolution, galaxies: ISM},
	Month = mar,
	Pages = {3363-3381},
