	Date-Added = {2019-07-26 16:13:38 -0700},
	Date-Modified = {2019-07-26 16:13:38 -0700},
	Doi = {10.1088/0004-637X/708/1/26},
	Eprint = {0911.0447},
	Journal = {\apj},
	Keywords = {galaxies: individual: NGC 4449, galaxies: starburst, galaxies: star clusters, H II regions, ISM: lines and bands},
	Month = jan,
	Pages = {26-37},
	Title = {{The Importance of Nebular Continuum and Line Emission in Observations of Young Massive Star Clusters}},
