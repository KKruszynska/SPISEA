	Date-Added = {2020-04-24 11:41:20 -0700},
	Date-Modified = {2020-04-24 11:41:21 -0700},
	Eprint = {astro-ph/9906332},
	Journal = {\aap},
	Keywords = {GALAXIES: EVOLUTION, GALAXIES: GENERAL, Astrophysics},
	Month = oct,
	Pages = {381-398},
	Primaryclass = {astro-ph},
	Title = {{Galaxy modelling. I. Spectral energy distributions from far-UV to sub-mm wavelengths}},
	Volume = {350},
	Year = 1999,
