	Eprint = {0911.3394},
	Journal = {\apj},
	Keywords = {globular clusters: general, methods: N-body simulations, stars: luminosity function, mass function, stellar dynamics},
	Month = jan,
	Pages = {1598-1610},
	Title = {{Tidal Disruption, Global Mass Function, and Structural Parameter Evolution in Star Clusters}},
