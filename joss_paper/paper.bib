	Eprint = {0906.4459},
	Journal = {\mnras},
	Keywords = {stellar dynamics , galaxies: star clusters},
	Month = oct,
	Pages = {141-156},
	Title = {{On the dissolution of star clusters in the Galactic Centre - I. Circular orbits}},
