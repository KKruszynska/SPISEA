	Date-Modified = {2018-12-22 05:43:12 -0800},
	Eprint = {1812.00014},
	Journal = {arXiv e-prints},
	Keywords = {Astrophysics - Astrophysics of Galaxies},
	Month = nov,
	Title = {{A systematic analysis of star cluster disruption by tidal shocks -- I. Controlled N-body simulations and a new theoretical model}},
