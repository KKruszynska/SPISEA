	Date-Modified = {2017-09-26 04:26:20 +0000},
	Doi = {10.1093/mnras/stt1633},
	Eprint = {1308.6436},
	Journal = {\mnras},
	Keywords = {open clusters and associations: general, galaxies: star clusters: general},
	Month = dec,
	Pages = {1116-1122},
	Title = {{The main sequence of three red supergiant clusters}},
	Volume = 436,
