	Author = {{Nieva}, M.-F. and {Sim{\'o}n-D{\'{\i}}az}, S.},
	Date-Added = {2017-09-12 02:11:53 +0000},
	Date-Modified = {2017-09-12 02:11:53 +0000},
	Doi = {10.1051/0004-6361/201116478},
	Eid = {A2},
	Eprint = {1104.3154},
	Journal = {\aap},
	Keywords = {stars: abundances, stars: atmospheres, stars: fundamental, parameters, open clusters and associations: general, stars:, early-type},
	Month = aug,
	Pages = {A2},
	Primaryclass = {astro-ph.SR},
	Title = {{The chemical composition of the Orion star forming region. III. C, N, Ne, Mg, and Fe abundances in B-type stars revisited}},
