@article{Webb:2017pw,
	Abstract = {We make use of N-body simulations to determine the relationship between two observable parameters that are used to quantify mass segregation and energy equipartition in star clusters. Mass segregation can be quantified by measuring how the slope of a cluster's stellar mass function alpha changes with clustercentric distance r, and then calculating delta _alpha = d alpha (r)/d ln(r/r_m), where rm is the cluster's half-mass radius. The degree of energy equipartition in a cluster is quantified by eta, which is a measure of how stellar velocity dispersion sigma depends on stellar mass m via sigma(m) ∝ m-eta. Through a suite of N-body star cluster simulations with a range of initial sizes, binary fractions, orbits, black hole retention fractions, and initial mass functions, we present the co-evolution of deltaalpha and eta. We find that measurements of the global eta are strongly affected by the radial dependence of sigma and mean stellar mass and the relationship between eta and deltaalpha depends mainly on the cluster's initial conditions and the tidal field. Within rm, where these effects are minimized, we find that eta and deltaalpha initially share a linear relationship. However, once the degree of mass segregation increases such that the radial dependence of sigma and mean stellar mass become a factor within rm, or the cluster undergoes core collapse, the relationship breaks down. We propose a method for determining eta within rm from an observational measurement of deltaalpha. In cases where eta and deltaalpha can be measured independently, this new method offers a way of measuring the cluster's dynamical state.},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2017MNRAS.464.1977W},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arxiv.org/abs/1609.09082},
	Author = {{Webb}, J.~J. and {Vesperini}, E.},
	Date-Added = {2017-01-16 21:04:52 +0000},
	Date-Modified = {2017-01-16 21:04:53 +0000},
	Doi = {10.1093/mnras/stw2513},
	Eprint = {1609.09082},
	Journal = {\mnras},
	Keywords = {methods: statistical, stars: statistics, globular clusters: general},
	Month = jan,
	Pages = {1977-1983},
	Title = {{On the link between energy equipartition and radial variation in the stellar mass function of star clusters}},
	Volume = 464,
	Year = 2017,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1093/mnras/stw2513},
